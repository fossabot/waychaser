{
  "name": "@mountainpass/waychaser",
  "version": "1.1.0",
  "description": "Client library for RESTful APIs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mountain-pass/waychaser.git"
  },
  "bugs": {
    "url": "https://github.com/mountain-pass/waychaser/issues"
  },
  "homepage": "https://github.com/mountain-pass/waychaser",
  "author": "Mountain Pass <info@mountain-pass.com.au>",
  "contributors": [
    "Tom Howard <tom@mountain-pass.com.au>"
  ],
  "license": "Apache-2.0",
  "main": "dist/waychaser.js",
  "module": "src/waychaser.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "config": {
    "DEV_UI_PORT": 3000,
    "DEV_API_PORT": 9000,
    "TEST_BROWSER_PORT": 5000
  },
  "scripts": {
    "env": "env",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --delete-dir-on-start --source-maps true --verbose --ignore src/test --out-dir dist src",
    "build:umd": "webpack --env OUTPUT_FILENAME=waychaser.umd.js",
    "build:umd.min": "webpack --mode production --env OUTPUT_FILENAME=waychaser.umd.min.js --env NODE_ENV=production",
    "prepack": "npm run build",
    "webpack:stats": "NODE_ENV=production webpack --mode production --env --profile --json > stats.json && open https://chrisbateman.github.io/webpack-visualizer/ && open http://webpack.github.io/analyse/",
    "browser:base": "NODE_ENV=test webpack serve --mode development --devtool inline-source-map --hot",
    "browser:dev": "npm run browser:base -- --port ${npm_package_config_DEV_UI_PORT} --env API_PORT=${npm_package_config_DEV_API_PORT} --hot",
    "browser:test": "NODE_ENV=test  npm run browser:base -- --port ${npm_package_config_TEST_BROWSER_PORT} --env API_PORT=${npm_package_config_TEST_API_PORT} --open=false",
    "lint": "eslint --fix",
    "pretest:node-api": "npm run build:main",
    "test:node-api": "NODE_ENV=test PORT=${npm_package_config_TEST_API_PORT} TEST_PROFILE=node-api cucumber-js -p node-api",
    "test:browser-api:subtask": "NODE_ENV=test PORT=${npm_package_config_TEST_API_PORT} UI_PORT=${npm_package_config_TEST_BROWSER_PORT} TEST_PROFILE=browser-api cucumber-js -p browser-api",
    "test:browser-api": "start-server-and-test browser:test http://localhost:${npm_package_config_TEST_BROWSER_PORT} test:browser-api:subtask",
    "test": "npm-run-all --sequential test:*",
    "watch:test:node-api": "nodemon -V -x npm -- run test:node-api",
    "watch:test:browser-api": "nodemon -V -x npm -- run test:browser-api",
    "watch:test": "nodemon -V -x npm -- run test",
    "cover:node-api": "nyc --nycrc-path .nycrc-node-api.yml --temp-dir coverage/node-api/.nyc_output --report-dir coverage/node-api npm run test:node-api",
    "cover:browser-api:subtask": "nyc --nycrc-path .nycrc-browser-api.yml --temp-dir coverage/browser-api/.nyc_output --report-dir coverage/browser-api npm run test:browser-api:subtask",
    "cover:browser-api": "start-server-and-test browser:test http://localhost:${npm_package_config_TEST_BROWSER_PORT} cover:browser-api:subtask",
    "cover:subtask": "scripts/merge-coverage.js && nyc report --report-dir coverage/full",
    "cover": "npm-run-all --sequential cover:node-api cover:browser-api cover:subtask",
    "eslint-check": "eslint --print-config src/server/index.js | eslint-config-prettier-check",
    "precommit": "lint-staged && npm run test:node-api",
    "npm-check-unused": "depcheck",
    "dopublish": "npm publish mountainpass-waychaser-${npm_package_version}.tgz --access public"
  },
  "files": [
    "/dist/*.js",
    "/dist/*.js.map"
  ],
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "debug": "^4.1.1",
    "http-link-header": "^1.0.2",
    "url-polyfill": "^1.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.5.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@windyroad/cucumber-js-throwables": "^1.0.4",
    "@windyroad/quick-containers-js": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^86.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "cucumber": "^5.1.0",
    "cucumber-junit-formatter": "^0.2.2",
    "depcheck": "^0.9.1",
    "dockerode": "^2.5.8",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^23.0.0",
    "express": "^4.16.3",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.5.0",
    "humanize-duration": "^3.24.0",
    "husky": "^4.2.5",
    "imagemin-lint-staged": "^0.4.0",
    "istanbul": "^0.4.5",
    "istanbul-middleware": "^0.2.2",
    "lint-staged": "^10.2.11",
    "ngrok": "^3.2.2",
    "nodemon": "^2.0.4",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.0",
    "prettier-config-standard": "^1.0.1",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "start-server-and-test": "^1.10.6",
    "webpack": "^4.5.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
